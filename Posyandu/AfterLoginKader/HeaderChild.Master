<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="HeaderChild.master.cs" Inherits="Posyandu.AfterLoginKader.HeaderChild" %>

<!DOCTYPE html>
<html>
<head runat="server">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Posyandu Dashboard</title>
 
    <link href="../Content/styles.css" rel="stylesheet" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
 <body>
     <form id="form1" runat="server">
<header class="main-header">
               <div class="left-header">
                <img src="../Content/Posyandu%202021%20Logo%20(PNG720p)%20-%20Vector69Com%201.png" class="WebLogo">
                <div class="navText">
                    <ul>
                       <b class ="This">Homepage</b>
                        <b class ="This">Profile Kader</b>
                    </ul>
                </div>
            </div>
                    <div class="profile">
                        <asp:Label ID="LabelName" runat="server" Text="Nama"></asp:Label>
                        <img src="../Content/milos%20.png" alt="Profile Picture" class="profile-pic">
                    </div>
                </header>
                
        <div class="container">
            <aside class="sidebar">
                <nav class="sidebar-nav">
                    <ul>
                        <li>
                            <img src="../Content/pngwing%201.png" style="width: 37px" class="logoNav" /><a href="WebForm1Kader.aspx" style="font-weight: bold; font-size: 15pt;">Halaman Utama</a></li>
                        <li>
                            <img src="../Content/pngwing%202.png" style="width: 36px; height: 33px" class="logoNav"/> <asp:HyperLink ID="LinkPenimbangan" runat="server" NavigateUrl='<%# Eval("NIK", "ControlPenimbangan.aspx?NIK={0}") %>' Text='<%# Eval("namaAnak") %>'>Penimbangan</asp:HyperLink></li>
                        <li>
                            <img src="../Content/children-kids-icon.png" style="width: 36px; height: 29px" class="logoNav"/><asp:HyperLink ID="LinkGigi" runat="server" NavigateUrl='<%# Eval("NIK", "ControlGigi.aspx?NIK={0}") %>' Text='<%# Eval("namaAnak") %>'>Kesehatan Gigi</asp:HyperLink></li>
                        <li>
                            <img src="../Content/pngwing%209.png" style="height: 36px; width: 39px" class="logoNav"/><asp:HyperLink ID="LinkCacing" runat="server" NavigateUrl='<%# Eval("NIK", "ControlCacing.aspx?NIK={0}") %>' Text='<%# Eval("namaAnak") %>'>Obat Cacing</asp:HyperLink></li>
                        <li>
                            <img src="../Content/pngwing%2012.png" style="height: 36px; width: 39px" class="logoNav"/><asp:HyperLink ID="LinkVitamin" runat="server" NavigateUrl='<%# Eval("NIK", "ControlVitamin.aspx?NIK={0}") %>' Text='<%# Eval("namaAnak") %>'>Vitamin A</asp:HyperLink></li>
                        
                    </ul>
                </nav>
            </aside>
            <main class="main-content">
                <section class="content">
                    <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                    </asp:ContentPlaceHolder>
                </section>
            </main>
        </div>
    </form>
</body>
</html>